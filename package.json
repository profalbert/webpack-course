{
  "name": "webpack-course",
  "version": "1.0.0",
  "description": "Learning webpack for front-end development",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode development",
    "dev": "cross-env NODE_ENV=development npx webpack --mode development",
    "dev:watch": "cross-env NODE_ENV=development npx webpack --mode development --watch",
    "build": "cross-env NODE_ENV=production npx webpack --mode production",
    "stats": "npx webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Albert Bekhter",
  "license": "ISC",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "csv-loader": "^3.0.3",
    "eslint": "^7.19.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.17.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
